local gui = Instance.new("ScreenGui", game.CoreGui)
local label = Instance.new("TextLabel", gui)
label.Size = UDim2.new(0, 400, 0, 40)
label.Position = UDim2.new(0.5, -200, 0, 40)
label.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
label.TextColor3 = Color3.new(1, 0, 0)
label.TextScaled = true
label.Font = Enum.Font.GothamBold
label.BorderSizePixel = 0
label.Visible = false

local found = false

for _, obj in pairs(game:GetDescendants()) do
    if (obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction")) and not obj:IsDescendantOf(game.Players) then
        if not found then
            label.Text = "üì° ÿ™ŸÖ ÿßŸÑŸÉÿ¥ŸÅ ÿπŸÜ Backdoor: " .. obj:GetFullName()
            label.Visible = true
            found = true
        end
    end
end
