btnSteal.Position = UDim2.new(0, 10, 0, 140)
btnSteal.Text = "ðŸ¦„ Ø³Ø­Ø¨ Ø£Ø¯ÙˆØ§Øª (Ø¶ØºØ·Ø© ÙˆØ­Ø¯Ø©)"
