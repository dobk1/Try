-- ØªÙ†ÙÙŠØ° Ø§Ù„Ø·ÙŠØ±Ø§Ù†
btnFly.MouseButton1Click:Connect(function()
    local floatPos = Vector3.new(9999, 5000, 9999)
    LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(floatPos))
    task.wait(0.2)
    if hrp then hrp.Anchored = true end
end)

-- ØªØ¬Ù…ÙŠØ¹ Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† ÙÙˆÙ‚Ùƒ
btnGather.MouseButton1Click:Connect(function()
    local basePos = hrp.Position + Vector3.new(0, 5, 0)
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            plr.Character.HumanoidRootPart.CFrame = CFrame.new(basePos + Vector3.new(math.random(-3,3), 0, math.random(-3,3)))
        end
    end
end)

-- Ø³Ø­Ø¨ Ø£Ø¯ÙˆØ§Øª Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† (ÙˆÙ‡Ù…ÙŠ ÙÙ‚Ø· Ø¹Ù†Ø¯Ùƒ)
btnGrabAllTools.MouseButton1Click:Connect(function()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            local backpack = plr:FindFirstChild("Backpack")
            if backpack then
                for _, tool in pairs(backpack:GetChildren()) do
                    if tool:IsA("Tool") then
                        local clone = tool:Clone()
                        clone.Name = "[ğŸ’Ù…Ø³Ø±ÙˆÙ‚] " .. tool.Name
                        clone.Parent = LocalPlayer.Backpack
                    end
                end
            end
            if plr.Character then
                for _, tool in pairs(plr.Character:GetChildren()) do
                    if tool:IsA("Tool") then
                        local clone = tool:Clone()
                        clone.Name = "[ğŸ¤šÙ…Ø³Ø±ÙˆÙ‚] " .. tool.Name
                        clone.Parent = LocalPlayer.Backpack
                    end
                end
            end
        end
    end
end)

-- Ø³Ø­Ø¨ Ø§Ù„ÙƒÙ„Ø¨Ø´Ø§Øª ÙØ¹Ù„ÙŠØ§Ù‹ (Clone Ù…Ù† Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†)
btnStealCuffs.MouseButton1Click:Connect(function()
    local targetToolName = "Handcuffs" -- ØºÙŠØ±Ù‡ Ø¥Ø°Ø§ Ø§Ø³Ù… Ø§Ù„ÙƒÙ„Ø¨Ø´Ø© Ù…Ø®ØªÙ„Ù
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            local backpack = plr:FindFirstChild("Backpack")
            if backpack then
                for _, tool in pairs(backpack:GetChildren()) do
                    if tool:IsA("Tool") and tool.Name == targetToolName then
                        local clone = tool:Clone()
                        clone.Name = "[ğŸ”’ÙƒÙ„Ø¨Ø´Ø©]" .. tool.Name
                        clone.Parent = LocalPlayer.Backpack
                    end
                end
            end
            if plr.Character then
                for _, tool in pairs(plr.Character:GetChildren()) do
                    if tool:IsA("Tool") and tool.Name == targetToolName then
                        local clone = tool:Clone()
                        clone.Name = "[ğŸ”’ÙƒÙ„Ø¨Ø´Ø©]" .. tool.Name
                        clone.Parent = LocalPlayer.Backpack
                    end
                end
            end
        end
    end
end)
