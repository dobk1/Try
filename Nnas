-- ÙˆØ¸Ø§Ø¦Ù Ø§Ù„Ø£Ø²Ø±Ø§Ø±
btnFly.MouseButton1Click:Connect(function()
    local floatPos = Vector3.new(9999, 5000, 9999)
    LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(floatPos))
    task.wait(0.2)
    if hrp then hrp.Anchored = true end
end)

btnGather.MouseButton1Click:Connect(function()
    local basePos = hrp.Position + Vector3.new(0, 5, 0)
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            plr.Character.HumanoidRootPart.CFrame = CFrame.new(basePos + Vector3.new(math.random(-3,3), 0, math.random(-3,3)))
        end
    end
end)

btnSteal.MouseButton1Click:Connect(function()
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= LocalPlayer and targetPlayer:FindFirstChild("Backpack") then
            for _, tool in ipairs(targetPlayer.Backpack:GetChildren()) do
                if tool:IsA("Tool") then
                    tool.Parent = LocalPlayer.Backpack
                end
            end
        end
    end
end)

btnFakeStack.MouseButton1Click:Connect(function()
    local offset = hrp.CFrame.LookVector * 5 -- Ù‚Ø¯Ø§Ù… Ø§Ù„Ù„Ø§Ø¹Ø¨ 5 ÙˆØ­Ø¯Ø§Øª
    local targetPos = hrp.Position + offset

    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local theirHRP = plr.Character.HumanoidRootPart
            theirHRP.CFrame = CFrame.new(targetPos, hrp.Position) -- ÙŠÙˆØ§Ø¬Ù‡Ùƒ
            -- Ø¨Ø¯ÙˆÙ† ØªØ«Ø¨ÙŠØª
        end
    end
end)
